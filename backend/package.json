{
  "name": "backend",
  "version": "1.0.0",
  "description": "This directory will contain the Node.js backend for the Scan & Sew App, using Apollo Server, Prisma, and PostgreSQL.",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "dev": "nodemon index.js",
    "test": "jest --verbose --coverage",
    "test:watch": "jest --watch",
    "lint": "eslint . --ext .js,.ts",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "docs:jsdoc": "jsdoc -c jsdoc.config.json",
    "docs:serve": "serve -s docs/jsdoc",
    "docs:swagger": "node scripts/generate-swagger.js",
    "postinstall": "prisma generate",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:studio": "prisma studio",
    "seed": "node prisma/seed.js",
    "reset-db": "node scripts/reset-db.js",
    "init-minio": "node scripts/init-minio.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.540.0",
    "@aws-sdk/credential-providers": "^3.844.0",
    "@aws-sdk/s3-request-presigner": "^3.540.0",
    "@graphql-tools/schema": "^10.0.23",
    "@prisma/client": "^6.11.1",
    "apollo-server-express": "^3.13.0",
    "bcryptjs": "^3.0.2",
    "cors": "^2.8.5",
    "dotenv": "^17.1.0",
    "express": "^4.21.2",
    "graphql": "^16.11.0",
    "graphql-middleware": "^6.1.35",
    "graphql-upload": "^13.0.0",
    "jsonwebtoken": "^9.0.2",
    "lodash.merge": "^4.6.2",
    "pg": "^8.16.3",
    "prisma": "^6.11.1",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.11",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.10.5",
    "@types/supertest": "^6.0.2",
    "@types/uuid": "^9.0.7",
    "better-docs": "^2.7.2",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "form-data": "^4.0.3",
    "jest": "^29.7.0",
    "jsdoc": "^4.0.2",
    "node-fetch": "^2.7.0",
    "nodemon": "^3.0.1",
    "prettier": "^3.1.1",
    "prisma": "^6.11.1",
